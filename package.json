{
  "name": "@nekofar/warpcast",
  "version": "2.0.0-alpha.10",
  "description": "TypeScript client for interacting with Warpcast APIs",
  "keywords": [
    "warpcast",
    "farcaster"
  ],
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "pnpm clean && pnpm openapi:validate && pnpm openapi:bundle && pnpm openapi:generate",
    "build": "tsup",
    "clean": "rimraf dist openapi.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "openapi:bundle": "swagger-cli bundle openapi/index.yaml --outfile openapi.json --type json",
    "openapi:generate": "openapi-ts",
    "openapi:validate": "swagger-cli validate openapi/index.yaml",
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build",
    "test": "echo 'No tests available.' && exit 0"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.8.4"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.64.15",
    "@types/node": "^22.13.14",
    "@typescript-eslint/utils": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-jsdoc": "^1.3.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.10",
    "rimraf": "^6.0.1",
    "swagger-cli": "^4.0.4",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  },
  "packageManager": "pnpm@10.0.0",
  "publishConfig": {
    "access": "public"
  }
}
